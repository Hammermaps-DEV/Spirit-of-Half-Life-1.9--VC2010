//Goal pathing entity -Sniper
@PointClass base(Targetname, Target) = info_monster_goal : "Monster goal entity" 
[
	spawnflags(Flags) =
	[
		8  : "Start Off" : 0
		16 : "Multithreaded" : 0 //If any monster reaches a goal, any other monsters with that goal will not need to get there. (If this isn't checked)
		32 : "Toggle On Reach" : 0 //Turns off this entity for all or a specific monster when reached
	]
	priority(Choices) : "Priority" : 0 =
	[
		0 : "No enemy"			//Will run to goal if not in combat with an enemy.
		1 : "No enemy/Enemy occluded"	//Will run to goal if not in combat, or in combat and enemy is hidden.
		//2,3,4 will still work if the monster does not have an enemy.
		2 : "Ignore enemy"		//Ignores enemies and just tries to make it to the goal point
		3 : "Goal closer than enemy"	//If this goal is closer than the enemy, just run to the goal.  	
		4 : "G.C.T.E./Enemy occluded"	//Same as 3, OR if the enemy isn't visible
		5 : "Stop only if can attack"	//Ignore all enemies unless you're able to attack something
	]
	movementtype(Choices) : "Movement Type" : 0 =
	[
		0 : "Walk" //How the monster will move to this goal
		1 : "Run"
	]
	delay(integer)	: "Wait before trigger"		: 0	//The monster must wait this long (and live) before the goal becomes inactive / targets are fired
	radius(integer)	: "Move-Complete Radius"	: 0	//When in radius, finished moving to the goal 	

	//if( in triggerradius and in radius of specific entity, do not attempt to go to other goal entities
	// (in other words, trigger radius ensures a monster goes to a specific entity if outside of it 
	//- if inside, does not count against the entity for going in favor of another )

	triggerradius(integer)	: "Distance For Inactivity"	: 0	//If closer to the goal than this, this goal is ignored.
	requiredradius(integer)	: "Maximum Required Distance"	: 0	//If we're farther than this amount, this goal is ignored.

	healthbonus(integer)	: "Health Bonus" 		: 0	//Amount of health the monster is given when completing a goal (limited to maximum health)
	sequence(string)	: "Action Animation"		: 0	//Play an animation before triggering stuff
	enemy(string)		: "Target Enemy"		//Becomes monster's enemy when completing the path
]

//Choice Priority - same name, higher priority, use if conditions meet.
//alarm - on see enemy, run to alarm called waypointA (priority 2),  priority 1 has same name, but conditions are (if no enemy)


//can be triggered on and off.
//if off, monster will search for another active waypoint of the same name. If nothing is found, the monster will stop moving.
@PointClass base(Targetname) = path_patrol : "Monster path entity" 
[
	spawnflags(Flags) =
	[
		8  : "Start Off"	: 0	//If checked, this point starts in-active and can not be used until triggered.
		16 : "Entry Point"	: 0	//This patrol point will be used for the start if checked
		32 : "Toggle On Reach"	: 0	//Turns off this entity for all or a specific monster when reached
	]

	target(target_destination)  : "Next Waypoint"
	message(target_destination) : "Target On Pass"

	movementtype(Choices) : "Movement Type" : 0 =
	[
		0 : "Walk" //How the monster will move to this waypoint
		1 : "Run"
	]

	approachtype(Choices) : "Approach Conditions" : 0 = 
	[
		0 : "No enemy"			//Will run to goal if not in combat with an enemy.
		1 : "No enemy in sight"		//Will run to goal if not in combat, or if in combat and enemy is hidden.
		2 : "Ignore enemies"		//Never stops to attack. Ignores enemies and just tries to make it to the goal point
		3 : "Goal Is Closer Than Enemy"	//If this goal is closer than the enemy, the NPC just runs to the goal.  	
		4 : "G.C.T.E./Enemy occluded"	//Same as 3, OR if the enemy isn't visible
		5 : "If can't attack"		//Ignores all enemies unless it can attack something
		6 : "Has Enemy"			//This point is used only if the NPC sees an enemy, or has an enemy. E.g. to set off alarms.
		7 : "Has New Enemy"		//This point is used only if the NPC sees a new enemy.
		8 : "Has Enemy & Can See Enemy"	//If this monster has an enemy and can see the enemy, then go to the goal.
	]

	delay(integer)		: "Wait" : 0		//The monster must wait this long before moving to next goal, and before any targets are fired.
	animation(string)	: "Action Animation"	//Animation to play on arrival. Target On Pass is fired after animation completes if animation is present.
	radius(integer)		: "Goal Radius" : 0	//When in radius, finished moving to the goal 	
	healthbonus(integer)	: "Health Bonus" : 0	//Amount of health the monster is given when completing a goal (limited to maximum health)
	enemyname(string)	: "Target Enemy"	//Becomes monster's enemy when completing the path.

	limit(integer)		: "Max. monster count" : 0	//This point only accepts X number of monsters at a time. Value is de-incremented after a monster starts moving to next waypoint.
	overflow(string)	: "Overflow Waypoint"		//If "Next Waypoint" is full, go to this waypoint instead.
	waituntilfull(choices)	: "Wait until full" : 0 =	//Wait until waypoint is full before proceeding to the next waypoint
	[
		0 : "No"
		1 : "Yes"
	]

	path_group(string)	: "Path Group Name"		//Group this point belongs to
	path_priority(integer)	: "Patrol Priority Level" : 0	//Priority level of THIS patrol point in the group.
]

@PointClass base(Targetname) color(255 128 0) = path_group : "Path Group" 
[
	//Game logic cycles through all path_groups with the same name, returns the highest priority group that is active.
	// THEN ----->
	//Game logic cycles through all path_patrols with the same group name, that are flagged with "Start point", and returns the highest priority point that is active.

	aliasname(string) : "(NPC Only) Group Name" //The name that NPCs search for

	//0 would be the default priority, followed by 1, 2, 3, etc. Higher active priorities are returned to the calling NPC,
	//unless there are no active paths in that path group, then it tries to find a lower priority group and so on.

	spawnflags(Flags) =
	[
		8  : "Start Off"   : 0	//If checked, this path group starts in-active and can not be used until triggered.
	]

	path_start(string) : "Starting point"
	requiredrange(integer) : "Active Radius" : 0 //0 = infinite. If not within specified radius, group doesn't work.
	//This path group is defined as what priority level?
	//Groups with the same name are picked first if their priority level is highest, and they're active (on).
	priority(integer) : "Group Priority Level" : 0
	
	selecttype(Choices) : "Point Selection" : 0 = 
	[
		0 : "Pick nearest in group"
		1 : "Pick the entry point"	//Will pick the first waypoint flagged with "Entry Point"
	]
]

